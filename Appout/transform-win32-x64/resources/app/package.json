{
  "name": "rdf2json",
  "productName": "rdf2json",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "src/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "packager-64": "electron-packager ./ transform --platform=win32  --arch=x64  --out=./Appout --overwrite",
    "packager-32": "electron-packager ./ transform --platform=win32  --arch=ia32  --out=./Appout --overwrite",
    "packageLinux-64": "electron-packager ./ transform --platform=linux --arch=x64 --out=./Appout --overwrite",
    "packageOS-64": "electron-packager ./ transform --platform=darwin --arch=x64 --out=./Appout --overwrite",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint src --color"
  },
  "keywords": [],
  "author": "DELL",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "rdf2json"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "rdf2json"
      }
    }
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "electron-compile": "^6.4.3",
    "electron-prebuilt-china": "^1.4.13",
    "electron-settings": "^3.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "zeromq": "^4.6.0",
    "zerorpc": "^0.9.7"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.2",
    "electron-prebuilt-compile": "2.0.7",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.10.0"
  }
}
